{
  "name": "caelumx-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:migrate": "prisma migrate dev --name init --schema=src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=src/prisma/schema.prisma",
    "prisma:seed": "ts-node src/prisma/seed.ts",
    "db:seed": "npm run prisma:seed"
  },
  "prisma": {
    "seed": "ts-node src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@helia/http": "^2.1.2",
    "@helia/unixfs": "^5.0.3",
    "@prisma/client": "^4.0.0",
    "@solana/web3.js": "^1.98.2",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helia": "^5.4.2",
    "ioredis": "^5.6.1",
    "ipfs-http-client": "^60.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.1",
    "redis": "^5.1.1",
    "ts-node-dev": "^2.0.0",
    "tweetnacl": "^1.0.3",
    "winston": "^3.17.0",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/bs58": "^4.0.4",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.129",
    "@types/pdfkit": "^0.13.9",
    "@types/redis": "^4.0.10",
    "prisma": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.0"
  }
}
